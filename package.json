{
  "name": "subtitlescode",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:electron": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && npm run electron\"",
    "electron:serve": "vite build && electron-builder --dir",
    "electron": "electron .",
    "electron:build": "vite build && electron-builder"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^36.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "vite": "^6.3.1",
    "vite-plugin-electron": "^0.29.0",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.subtitlescode",
    "icon": "public/favicon.ico",
    "files": [
      "dist/**/*",
      "./electron/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "extends": null
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@douyinfe/semi-icons": "^2.78.0",
    "@douyinfe/semi-icons-lab": "^2.78.0",
    "@douyinfe/semi-ui": "^2.78.0",
    "@emnapi/core": "^1.4.3",
    "@tailwindcss/vite": "^4.1.5",
    "antd": "^5.24.9",
    "daisyui": "^5.0.32",
    "framer-motion": "^12.9.4",
    "ogl": "^1.0.11",
    "postprocessing": "^6.37.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.87.0",
    "tailwindcss": "^4.1.5",
    "three": "^0.176.0"
  }
}
